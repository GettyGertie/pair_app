<h1>Sign Up</h1>
<% if @user.errors.any? %>
    <% @user.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
        <% end %>
    <% end %>
<%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <button>Sign Up</button>
<% end %>
